<h1>Search!</h1>

<nav class="users-search">
  <input id="users-search-input" type="text" name="found-users" value="">

  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= link_to user.username, user_url(user) %>
        <%= render "follows/form", user: user %>
      </li>
    <% end %>
  </ul>
</nav>
